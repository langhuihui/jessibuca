{
    "name": "jessibuca",
    "version": "1.0.0",
    "main": "dist/jessibuca.js",
    "module": "dist/jessibuca.esm.js",
    "unpkg": "dist/jessibuca.js",
    "jsdelivr": "dist/jessibuca.js",
